{
  "expo": {
    "name": "Money Bracket",
    "slug": "moneybracket",
    "scheme": "com.tusharyaar.mb",
    "version": "0.8.4",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "orientation": "default",
    "icon": "./assets/christmas-icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tusharyaar.moneybracket"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "versionCode": 5038,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/monochrome.png",
        "backgroundColor": "#f2f2f2"
      },
      "package": "com.tusharyaar.moneybracket"
    },
    "extra": {
      "eas": {
        "projectId": "7f819d50-7144-4e92-9f26-a4e970e6325d"
      }
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native",
        {
          "url": "https://sentry.io/",
          "project": "money-bracket",
          "organization": "tushar-s-agrawal",
          "experimental_android": {
            "enableAndroidGradlePlugin": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Lexend-Regular.ttf",
            "./assets/Lexend-Medium.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f2f2f2",
          "image": "./assets/adaptive-icon.png",
          "dark": {
            "image": "./assets/adaptive-icon.png",
            "backgroundColor": "#282c34"
          },
          "imageWidth": 200
        }
      ],
      "expo-sqlite",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": [
              "en",
              "hi",
              "es"
            ],
            "android": [
              "en",
              "hi",
              "es"
            ]
          }
        }
      ],
      "expo-asset"
    ]
  }
}
